{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "member-access": false,
        "object-literal-sort-keys": false,
        "ordered-impacts": false,
        "interface-name": false,
        "no-submodule-imports": false,
        "quotemark": [
          true,
          "single",
          "avoid-escape",
          "avoid-template"
        ],
        "semicolon": [
          true,
          "never"
        ],
        "trailing-comma": false,
        "arrow-parens": false
      },
    "rulesDirectory": []
}